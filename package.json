{
  "name": "project-devio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "migration:dev": "npm run dev:load:env prisma migrate dev",
    "migration:deploy": "npm run dev:load:env prisma migrate deploy",
    "dev:load:env": "dotenv -e .env.development",
    "dev": "npm run dev:load:env nodemon src/server.ts",
    "start": "prisma migrate deploy && node dist/src/server.js",
    "test:load:env": "dotenv -e .env.test",
    "test": "npm run test:load:env -- jest --passWithNoTest --runInBand",
    "test:unit": "npm run test:load:env -- jest --testMatch **/tests/unit/*.test.ts",
    "test:integration": "npm run test:load:env -- jest --testMatch **/tests/integration/*.test.ts",
    "test:seed": "npm run test:load:env prisma db seed",
    "test:coverage": "npm run test -- --coverage",
    "test:migration:run": "npm run test:load:env prisma migrate deploy",
    "test:migration:generate": "npm run test:load:env prisma migrate dev",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "precommit": "chmod +x .husky/pre-commit",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.7.3",
    "joi": "^17.11.0",
    "lint-staged": "^15.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@faker-js/faker": "^8.3.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
